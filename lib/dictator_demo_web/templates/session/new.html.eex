<h1>Sign In</h1>

<%= form_for @conn, Routes.session_path(@conn, :create), fn f -> %>
  <%= label f, :email %>
  <%= text_input f, :email %>
  <%= error_tag f, :email %>

  <%= label f, :password %>
  <%= password_input f, :password %>
  <%= error_tag f, :password %>

  <div>
    <%= submit "Sign In" %>
  </div>
<% end %>
